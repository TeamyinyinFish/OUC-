<!--pinform.wxml-->
<view class="face">
  <view class="example">
    <view class="marquee_box">
      <view class="marquee_text" style="{{orientation}}:{{marqueeDistance}}px;font-size: {{size}}px;">
        <image src="{{adUrl}}" class='ad-image' />{{text}}
      </view>
    </view>
  </view>
    <text class="head">OUC图书馆</text>
  <view>
      <label class="block" src="/pages/image/bg1.jpg">
      <view id="block1" bindtap="block1">
        <image id="one" src="/pages/image/check1.png"></image>
        <text id="nav1" >检索</text>
      </view>
      <view id="block2" bindtap="block2">
        <image id="two" src="/pages/image/renew1.png"></image>
        <text id="nav2" >借书</text>
      </view>
      <view id="block3" bindtap="block3">
        <image id="three" src="/pages/image/remark1.png"></image>
        <text id="nav3" >评论</text>
      </view>
      <view id="block4" bindtap="block4">
        <image id="four" src="/pages/image/appt1.png"></image>
        <text id="nav4">预约</text>
      </view>
      </label>
      <view class="foot">
        <view class="introduction" >
          <text id="txt1">图书馆简介</text>
          <image id="img1"  src="../image/nav1.png"/>

        </view>
        <view class="distribute" bindtap="gotoPage" >
          <text  id="txt2">馆内楼层分布</text>
          <image id="img2"  src="../image/nav1.png"/>
        </view>
        <view class="notice">
          <text id="txt3">通知</text>
          <image id="img3" src="../image/nav1.png"/>
        </view>
      </view>
  </view>
</view>